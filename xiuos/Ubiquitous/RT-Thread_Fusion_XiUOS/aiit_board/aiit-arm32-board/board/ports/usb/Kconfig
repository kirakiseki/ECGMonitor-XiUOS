config BSP_USING_STM32_USBH
    bool "Using usb host"
    default y
    if BSP_USING_STM32_USBH
        config USB_BUS_NAME
            string "usb bus name"
            default "usb"
        config USB_DRIVER_NAME
            string "usb bus driver name"
            default "usb_drv"
        config USB_DEVICE_NAME
            string "usb bus device name"
            default "usb_dev"
    endif

